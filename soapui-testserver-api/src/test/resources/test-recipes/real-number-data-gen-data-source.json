{
  "testSteps": [
    {
      "type": "DataSource",
      "name": "The DataSource",
      "dataSource": {
        "dataGen": {
          "numberOfRows": "5",
          "dataGenerators": [
            {
              "type": "Real",
              "propertyName": "property1",
              "duplicationFactor": 2,
              "minimumValue": 1.1,
              "maximumValue": 99
            },
            {
              "type": "Real",
              "propertyName": "property2",
              "generationMode": "Sequential",
              "minimumValue": 10.21,
              "maximumValue": 90.32,
              "decimalPlaces": 4,
              "incrementBy" : 1.2
            }
          ]
        }
      },
      "testSteps": [
        {
          "type": "REST Request",
          "method": "GET",
          "URI": "http://google.com/",
          "mediaType": "application/json",
          "parameters": [
            {
              "name": "q",
              "value": "${The DataSource#property1}"
            },
            {
              "name": "r",
              "value": "${The DataSource#property2}"
            }
          ],
          "assertions": [
            {
              "type": "Valid HTTP Status Codes",
              "validStatusCodes": [
                "200",
                "201"
              ]
            }
          ]
        }
      ]
    }
  ]
}

