{
  "testSteps": [
    {
      "type": "DataSource",
      "name": "The DataSource",
      "dataSource": {
        "file": {
          "file": "/tmp/foo.csv",
          "charset": "UTF-8",
          "separator": ",",
          "trim": true,
          "quotedValues": true
        },
        "properties": [
          "prop1",
          "prop2"
        ]
      },
      "testSteps": [
        {
          "type": "REST Request",
          "method": "GET",
          "URI": "http://google.com/",
          "mediaType": "application/json",
          "parameters": [
            {
              "name": "q",
              "value": "${The DataSource#prop1}"
            },
            {
              "name": "usr",
              "value": "${The DataSource#prop2}"
            }
          ]
        }
      ]
    }
  ]
}